{
  "private": true,
  "version": "0.0.0",
  "description": "Boilerplate project with React, Node, GraphQL, Prisma and TypeScript",
  "scripts": {
    "lint": "lerna run lint",
    "build:deps": "lerna run build --scope=@react-graphql-prisma-ts/common",
    "new-version": "lerna version --conventional-commits --yes"
  },
  "keywords": [
    "react",
    "graphql",
    "prisma",
    "typescript",
    "eslint",
    "prettier",
    "storybook",
    "jest",
    "cypress"
  ],
  "author": "Jonathan Robic <hello@jonathanrobic.fr> (https://jonathanrobic.fr)",
  "license": "MIT",
  "devDependencies": {
    "@react-graphql-prisma-ts/config": "0.0.1",
    "cz-conventional-changelog": "3.0.2",
    "lerna": "^3.16.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  }
}
